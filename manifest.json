{
  "name": "Unpink Pinterest",
  "version": "1.0",
  "content_scripts" : [
    {
      "matches": ["http://*.pinterest.com/*"],
      "js": ["unpink-pinterest.js"]
    }
  ],
  "manifest_version": 2,
  "description": "Change the color scheme of pinterest from red to blue.",
  "permissions": [
    "tabs"
  ]
}